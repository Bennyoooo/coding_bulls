<h2> Dashboard </h2>
<% if Add.all %>
<% user = User.find(params[:id]) %>
<% user.posts_added.each do |post| %>
 <div class="panel-heading">
    <h3 class="panel-title">
        <%= post.caption %>
    </h3>
    <%= link_to 'Show', post, class: "btn btn-info" %>
    <%= link_to 'Remove from Dashboard', unadds_path(post.id), method: :delete, class: "btn btn-info" %>
 </div>
 <% end %>
<% else %>
<h2> You currently do not have any posts added to your dashboard </h2>
<% end %>
